@page
@model EFCZealand.Pages.UddannelseOgLærerAllokering.OpretModel
@{
}

<h1 class="display-3 mb-5">Opret ny tid</h1>

<form method="post">

    <div class="form-group mb-3">
        <label asp-for="@Model.Element.Lærer.Navn" class="control-label">Lærer Navn</label>
        <input asp-for="@Model.Element.Lærer.Navn" class="form-control" />
        <span asp-validation-for="@Model.Element.Lærer.Navn" class="text-danger"></span>
    </div>

    <div class="form-group mb-3">
        <label asp-for="@Model.Element.Uddannelse.UddannelseNavn" class="control-label">Uddannelse Navn</label>
        <input asp-for="@Model.Element.Uddannelse.UddannelseNavn" class="form-control" />
        <span asp-validation-for="@Model.Element.Uddannelse.UddannelseNavn" class="text-danger"></span>
    </div>

    <div class="form-group mb-3">
        <label asp-for="@Model.Element.StartTid" class="control-label">Start Tid</label>
        <input asp-for="@Model.Element.StartTid" class="form-control" />
        <span asp-validation-for="@Model.Element.StartTid" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <label asp-for="@Model.Element.SlutTid" class="control-label">Slut Tid</label>
        <input asp-for="@Model.Element.SlutTid" class="form-control" />
        <span asp-validation-for="@Model.Element.SlutTid" class="text-danger"></span>
    </div>

    <p>
        <div class="form-group mb-3">
            <input type="submit" value="Opret" class="btn btn-outline-primary" />
        </div>
    </p>
</form>
